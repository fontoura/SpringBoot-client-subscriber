    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch SampleClientApplication",
            "request": "launch",
            "mainClass": "com.github.fontoura.mavensamples.publishandsubscribe.sample.SampleClientApplication",
            "projectName": "service-client-sample",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Launch WorkerApplication",
            "request": "launch",
            "mainClass": "com.github.fontoura.mavensamples.publishandsubscribe.worker.WorkerApplication",
            "projectName": "worker",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "java",
            "name": "Launch PublisherApplication",
            "request": "launch",
            "mainClass": "com.github.fontoura.mavensamples.publishandsubscribe.service.PublisherApplication",
            "projectName": "service-impl",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}